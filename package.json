    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "googleapis": "^128.0.0",
    "node-fetch": "^3.3.2",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "netlify-cli": "^17.10.1",
    "prettier": "^3.1.0"
